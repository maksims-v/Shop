{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 71,
      "minLength": 3,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "clothing",
        "footwear",
        "accessories",
        "activities"
      ],
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "price": {
      "type": "decimal",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "new": {
      "type": "boolean",
      "default": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "sale": {
      "type": "boolean",
      "default": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "oldPrice": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "brand": {
      "type": "string",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "color": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "color.color"
    },
    "size": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "size.size"
    },
    "gender": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "all",
        "men's",
        "women's"
      ],
      "required": true,
      "default": "all"
    },
    "clearance": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "default": false
    },
    "techDescription": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "subproductcategory": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "subproductcategory.subproductcategory"
    },
    "subcategory": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "all",
        "shoes",
        "socks",
        "sandals",
        "jackets",
        "shirts",
        "headwear",
        "bags",
        "gloves"
      ],
      "required": true
    },
    "Equipment": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "customField",
      "options": [
        "Tents",
        "Shelters",
        "Sleeping Bags",
        "Sleeping Pads",
        "Hammocks/Accessories",
        "Camping Furniture",
        "Pillows"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order.order",
      "inversedBy": "variation"
    }
  }
}
